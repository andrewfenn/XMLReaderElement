{
    "name": "andrewfenn/xmlreader",
    "description" : "andrewfenn/xmlreader is an XML library that makes sabre/xml package easier to use",
    "keywords" : [ "XML", "XMLReader", "XMLWriter", "DOM" ],
    "homepage" : "https://github.com/andrewfenn/XMLReaderElement",
    "license" : "BSD-3-Clause",
    "require" : {
        "php" : ">=5.5.5",
        "sabre/xml": "^1.5"
    },
    "authors" : [
        {
            "name" : "Andrew Fenn",
            "email" : "andrewfenn@gmail.com",
            "role" : "Developer"
        }
    ],
    "support" : {
        "source" : "https://github.com/andrewfenn/XMLReaderElement"
    },
    "autoload" : {
        "psr-4" : {
            "Sabre\\Xml\\" : "lib/"
        },
        "files": [
            "lib/XMLReaderElement.php"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Sabre\\Xml\\" : "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit" : "*"
    },
    "config" : {
        "bin-dir" : "bin/"
    }
}
